{
  "name": "note",
  "version": "1.0.1",
  "description": "This is my personal note",
  "type": "module",
  "scripts": {
    "docs:dev": "vitepress dev docs --port 7910",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "watchChangeRestartVitepress": "node ./docs/.vitepress/watchChangeRestartVitepress.mjs",
    "release": "release-it",
    "lint-fix": "eslint --fix"
  },
  "keywords": [],
  "license": "ISC",
  "homepage": "https://fang-kang.github.io/note/",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/react",
        "react",
        "react-dom",
        "@algolia/client-search"
      ]
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@release-it/conventional-changelog": "^5.1.1",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "chokidar": "^3.5.3",
    "eslint": "^8.36.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "flexsearch": "^0.7.31",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.0",
    "markdown-it": "^13.0.1",
    "markdown-it-custom-attrs": "^1.0.2",
    "prettier": "^2.8.4",
    "release-it": "^15.8.0",
    "typescript": "^4.9.5",
    "unocss": "^0.50.4",
    "vite": "^4.1.4",
    "vite-plugin-vitepress-demo": "2.0.0-alpha.8",
    "vitepress": "1.0.0-alpha.51",
    "vitepress-plugin-search": "1.0.4-alpha.19",
    "vue": "^3.2.47"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
